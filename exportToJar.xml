<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create runnable Jar for project AmstradPc">
    <target name="create_run_jar">
        <jar destfile="dist/amstradPc.jar">
            <manifest>
                <attribute name="Main-Class" value="org.maia.amstrad.pc.AmstradMain"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="bin"/>
        </jar>
    </target>
</project>